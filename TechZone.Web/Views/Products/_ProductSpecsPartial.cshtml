@model Dictionary<string, string>

<div class="row">
    <div class="col-xs-10 col-md-7 col-lg-5">
        <table class="table table-striped table-responsive">
            @{
                foreach (var kvp in Model)
                {
                    <tr>
                        <td><b>@kvp.Key</b></td>
                        <td>@Html.DisplayFor(m => kvp.Value)</td>
                    </tr>
                }
            }
        </table>
    </div>
</div>